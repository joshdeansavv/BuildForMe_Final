Host *
  AddressFamily inet
  GSSAPIAuthentication no
  # Connection persistence settings
  ServerAliveInterval 60
  ServerAliveCountMax 3
  TCPKeepAlive yes
  ControlMaster auto
  ControlPath ~/.ssh/control-%h-%p-%r
  ControlPersist 10m
  # Prevent connection sharing issues
  Multiplexing yes
  MaxSessions 10

Host home
  HostName 192.168.1.178
  User joshua
  IdentityFile ~/.ssh/id_ed25519
  IdentitiesOnly yes
  StrictHostKeyChecking accept-new 